<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#555555"
    tools:context=".DraftHistory" >

    <com.example.fantasyfootballrankings.InterfaceAugmentations.BounceListView
        android:id="@+id/draft_history_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:divider="#999999"
        android:layout_alignParentTop="true"
        android:background="@drawable/content_well">

    </com.example.fantasyfootballrankings.InterfaceAugmentations.BounceListView>

    <com.devspark.sidenavigation.SideNavigationView
        android:id="@+id/side_navigation_view_draft"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</RelativeLayout>
