<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ffr.fantasyfootballrankings"
    android:versionCode="33"
    android:versionName="3.4.1" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application 
        android:allowBackup="true"
        android:icon="@drawable/fb" 
        android:label="@string/app_name" 
        android:theme="@style/AppTheme" > 
        <activity 
            android:name="com.example.fantasyfootballrankings.Pages.Home"
             android:screenOrientation="portrait"
		   	android:clearTaskOnLaunch="true"
		   	android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name" >
            <intent-filter> 
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.fantasyfootballrankings.Pages.Rankings"
             android:screenOrientation="portrait"
		   	android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_rankings" >
        </activity>
        <activity
            android:name="com.example.fantasyfootballrankings.Pages.ImportLeague"
            android:screenOrientation="portrait"
		   	android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_import_league" >
        </activity>
    </application>

</manifest>